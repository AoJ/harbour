{
    "name" : "seaport",
    "description" : "service registry and port assignment for clusters",
    "version" : "1.0.0",
    "repository" : {
        "type" : "git",
        "url" : "git://github.com/substack/seaport.git"
    },
    "main" : "index.js",
    "browserify" : "lib/seaport.js",
    "bin" : {
        "seaport" : "bin/cli.js"
    },
    "keywords" : [
        "port",
        "allocate",
        "hub",
        "service",
        "registry",
        "roles"
    ],
    "directories" : {
        "lib" : ".",
        "example" : "example",
        "test" : "test"
    },
    "scripts" : {
        "test" : "tap test/*.js"
    },
    "dependencies" : {
        "optimist" : "~0.3.5",
        "semver" : "~1.1.0",
        "inherits" : "~1.0.0",
        "crdt" : "~3.1.3",
        "through" : "~1.1.1"
    },
    "devDependencies" : {
        "tap" : "~0.3.0"
    },
    "license" : "MIT",
    "author" : {
        "name" : "James Halliday",
        "email" : "mail@substack.net",
        "url" : "http://substack.net"
    }
}
